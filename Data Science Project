In [4]:	
#Change Stress Level Column to be Integers
def stress_level(level):
    if level == 'Low':
        return 1
    elif level == 'Moderate':
        return 2
    elif level == 'High': 
        return 3

df['Stress Level'] = df['Stress Level'].apply(stress_level)
df
